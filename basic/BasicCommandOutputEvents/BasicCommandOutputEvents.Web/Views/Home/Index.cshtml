@using InterAppConnector.DataModels
@model ResultViewModel

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Message returned by the command</h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Parameter</th>
                <th scope="col">Value</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th scope="row">MessageStatus</th>
                <td>@Model.CommandResult.MessageStatus</td>
            </tr>
            <tr>
                <th scope="row">MessageType</th>
                <td>@Model.CommandResult.MessageType</td>
            </tr>
            <tr style="background-color:@Model.Color">
                <th scope="row">Message</th>
                <td>@Model.CommandResult.Message</td>
            </tr>
        </tbody>
    </table>
</div>